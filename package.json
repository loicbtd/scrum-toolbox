{
  "name": "scrum-toolbox",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "typeorm": "typeorm",
    "postinstall": "exitzero electron-builder install-app-deps && ngcc --properties es2020 browser module main",
    "storybook": "nx run lib-angular:storybook",
    "start": "run-p start:app-desktop",
    "start:app-desktop": "run-p start:app-client-angular start:app-client-electron",
    "start:app-client-angular": "nx serve app-client-angular --configuration=development",
    "start:app-client-electron": "wait-on tcp:4200 && nx serve app-client-electron --configuration=development",
    "publish:step1": "nx run-many --target=build --configuration=production --projects=app-client-angular,app-client-electron --parallel=true",
    "publish:step2:windows": "nx run app-client-electron:make --configuration=production --arch x64 --platform windows",
    "publish:step2:macos": "nx run app-client-electron:make --configuration=production --arch x64 --platform mac",
    "publish:step2:linux:": "nx run app-client-electron:make --configuration=production --arch x64 --platform linux",
    "publish:windows": "npm-run-all publish:step1 publish:step2:windows",
    "publish:macos": "npm-run-all publish:step1 publish:step2:macos",
    "publish:linux": "npm-run-all publish:step1 publish:step2:linux"
  },
  "private": true,
