    "primeflex": "^3.1.0",
    "primeicons": "^4.1.0",
    "primeng": "^12.2.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "toad-scheduler": "^1.6.0",
    "tslib": "^2.4.0",
